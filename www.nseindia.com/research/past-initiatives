  <div class="accordLink text-end mb-2 staticAccord">
    <a onclick="expandAll('accordion-961nvmkrjh8')" href="javascript:;" aria-current="true">Expand All</a> |
    <a onclick="collapseAll('accordion-961nvmkrjh8')" href="javascript:;" aria-current="false">Collapse All</a>
