  <div class="card">
    <div class="card-header">
      <a class="card-link collapsed" aria-expanded="false" data-bs-toggle="collapse" href="#accordion-pirih5frh4g-2">NSE-NYU Stern Initiative 2020</a>
    </div>
    <div id="accordion-pirih5frh4g-2" class="collapse " data-bs-parent="#accordion-pirih5frh4g">
      <div class="card-body">
        
        <div>
          <table border="1" style="width: 100%;">
	<thead>
		<tr>
			<th class="text-align-center" scope="col">Sr. No.</th>
			<th class="text-align-center" scope="col">Title</th>
			<th class="text-align-center" scope="col">Author</th>
			<th class="text-align-center" scope="col">Presentation</th>
			<th class="text-align-center" scope="col">Working paper</th>
			<th class="text-align-center" scope="col">White Paper</th>
		</tr>
